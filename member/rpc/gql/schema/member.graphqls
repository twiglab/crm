type MemberResp {
  code: String!
  wxOpenID: String!
}

type QryMemberResp {
  code: String!
  wxOpenID: String!

  found:Boolean!
}

input OpenIDReq {
  wxOpenID: String!
}

input CreateWxMemberReq {
  code: String!
  wxOpenID: String!
}

type Query {
  queryWxMemberByOpenID (input: OpenIDReq!): QryMemberResp!
}


type Mutation {
  createWxMember(input: CreateWxMemberReq!): MemberResp!
  wxLogin(input: OpenIDReq!): QryMemberResp!
}
