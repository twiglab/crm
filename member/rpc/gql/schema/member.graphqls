type MemberResp {
  code: String!
  wxOpenID: String!
}

input OpenIDReq {
  wxOpenID: String!
}

input CreateWxMemberReq {
  code: String!
  wxOpenID: String!
}

type Query {
  QueryWxMember (input: OpenIDReq!): MemberResp
  QueryMember (input: OpenIDReq!): MemberResp
  GetMember (input: OpenIDReq!): MemberResp
}


type Mutation {
  CreateWxMember(input: CreateWxMemberReq!): MemberResp!
}
