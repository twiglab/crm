type MemberResp {
  code: String!
  wxOpenID: String!
}

input OpenIDReq {
  wxOpenID: String!
}

input CreateWxMemberReq {
  code: String!
  wxOpenID: String!
}

type Query {
  queryWxMemberByOpenID (input: OpenIDReq!): MemberResp
}


type Mutation {
  createWxMember(input: CreateWxMemberReq!): MemberResp!
}
